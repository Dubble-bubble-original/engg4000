{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "engg4000-dev_nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client","backend"],
            "memory": 100
        },
        {
            "name": "client",
            "image": "engg4000-dev_client",
            "hostname": "client",
            "essential": false,
            "memory": 300
        },
        {
            "name": "backend",
            "image": "engg4000-dev_backend",
            "hostname": "backend",
            "essential": false,
            "memory": 300
        }
    ]
}
